module statixpie

imports

  lex/literal
  lex/layout
  lex/comment
  type
  data
  func_def
  func_body

template options

  keyword -/- [a-zA-Z0-9\-\_]

context-free sorts

  Start
  Module
  Import
  Def

context-free start-symbols

  Start

context-free syntax

  Start.Program =
  <<Module>

  <{Import "\n"}*>

  <{Def "\n"}*>>

  Module.Module = <module <{MODID ":"}+>>

  Import.Import = <import <{MODID ":"}+>>

  Def = DataDef
  Def = FuncDef
